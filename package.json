{
  "name": "@ueqt/uma",
  "version": "9.0.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:github": "ng build --prod --base-href=https://ueqt.github.io/uma/",
    "build:lib:scss": "scss-bundle -c scss-bundle.config.json",
    "docs": "ng deploy --base-href=https://ueqt.github.io/uma/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p src/lib/package.json && npm run build:lib:scss && cp README.md src/lib/dist/",
    "changelog": "npx conventional-changelog-cli -p angular -s -i CHANGELOG.md -r 0"
  },
  "peerDependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular-devkit/core": "^9.1.6",
    "@angular/animations": "^9.1.7",
    "@angular/cdk": "^9.2.3",
    "@angular/cli": "^9.1.6",
    "@angular/common": "^9.1.7",
    "@angular/compiler": "^9.1.7",
    "@angular/compiler-cli": "^9.1.7",
    "@angular/core": "^9.1.7",
    "@angular/forms": "^9.1.7",
    "@angular/language-service": "^9.1.7",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "^9.1.7",
    "@angular/platform-browser-dynamic": "^9.1.7",
    "@angular/router": "^9.1.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.2.2",
    "conventional-changelog": "^3.1.21",
    "core-js": "^3.6.5",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.5",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.5.4",
    "ng-packagr": "^9.1.3",
    "protractor": "~7.0.0",
    "rxjs": "^6.5.5",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3",
    "uglify-es": "^3.3.9",
    "zone.js": "~0.10.3"
  },
  "dependencies": {
    "scss-bundle": "^3.1.2",
    "tslib": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run changelog && git add CHANGELOG.md",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
